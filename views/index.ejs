<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css'/>
  <script src="/javascripts/main.js"></script>
</head>
<body>
<div class="container">
  <header>
    <h1><%= title %></h1>

    <p>
      音 量 : <input class="volume" type="range" min="0" max="100" value="60"/>
    </p>
  </header>
  <div class="left">
    <ul class="list">
      <% music.forEach(function (name) { %>
      <li><%= name %></li>
      <% }) %>
    </ul>
  </div>
  <div class="right"></div>
</div>
</body>
</html>
